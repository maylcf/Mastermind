            <solid android:color="@color/background5"/>
            <stroke android:width="5px" android:color="@color/background5" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="#b3b2a192"/>
            <stroke android:width="5px" android:color="#b3b2a192" />
        </shape>
    </item>

    <item android:state_enabled="true">
        <shape android:shape="oval">
            <stroke android:width="5px" android:color="@color/background5" />
